From ed5a8e8708f855bc1476634b7567241c800e9a4c Mon Sep 17 00:00:00 2001
From: Anuj Mittal <anujx.mittal@intel.com>
Date: Fri, 11 Mar 2016 10:06:18 +0800
Subject: [PATCH] security_flags.inc: blacklist recipes that don't compile

Add recipes that don't work with the security flags enabled.

Signed-off-by: Anuj Mittal <anujx.mittal@intel.com>
---
 meta/conf/distro/include/security_flags.inc | 2 ++
 1 file changed, 2 insertions(+)

diff --git a/meta/conf/distro/include/security_flags.inc b/meta/conf/distro/include/security_flags.inc
index 14ac0bf..a94d910 100644
--- a/meta/conf/distro/include/security_flags.inc
+++ b/meta/conf/distro/include/security_flags.inc
@@ -86,6 +86,8 @@ SECURITY_CFLAGS_pn-tcl = "${SECURITY_NO_PIE_CFLAGS}"
 SECURITY_CFLAGS_pn-tiff = "${SECURITY_NO_PIE_CFLAGS}"
 SECURITY_CFLAGS_pn-valgrind = "${SECURITY_NO_PIE_CFLAGS}"
 SECURITY_CFLAGS_pn-zlib = "${SECURITY_NO_PIE_CFLAGS}"
+SECURITY_CFLAGS_pn-mraa = "${SECURITY_NO_PIE_CFLAGS}"
+SECURITY_CFLAGS_pn-efivar = "${SECURITY_NO_PIE_CFLAGS}"
 
 # These 2 have text relco errors with the pie options enabled
 SECURITY_CFLAGS_pn-ltp = "${SECURITY_NO_PIE_CFLAGS}"
-- 
